{
  "name": "shippingmanager-copilot",
  "version": "0.1.4.2",
  "description": "Standalone web interface for Shipping Manager chat and game management",
  "main": "app.js",
  "bin": "app.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "echo 'Please run: python start.py' && exit 1",
    "docs": "jsdoc -c jsdoc.json",
    "prepare": "husky",
    "build": "node build/build.js",
    "build:python": "python -m PyInstaller build/build-start.spec",
    "build:python:helpers": "python -m PyInstaller build/build-python.spec",
    "build:node": "npx @yao-pkg/pkg . --targets node22-win-x64 --output dist/ShippingManagerCoPilot-Server.exe",
    "build:browser-login": "node build/build-browser-login.js",
    "build:package": "node build/build-package.js",
    "build:installer": "node build/build-installer.js",
    "build:all": "npm run build:python:helpers && npm run build:node && npm run build:python && npm run build:browser-login && npm run build:package && npm run build:installer",
    "lint": "eslint .",
    "bandit": "python -m bandit -r helper/ -c .bandit",
    "bandit:full": "python -m bandit -r helper/",
    "security": "npm run lint && npm run bandit"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "server/**/*",
      "sysdata/forecast/**/*",
      "node_modules/axios/**/*",
      "node_modules/ws/**/*",
      "node_modules/express/**/*",
      "node_modules/helmet/**/*",
      "node_modules/validator/**/*",
      "node_modules/node-forge/**/*",
      "package.json",
      "jsdoc.json"
    ],
    "scripts": [
      "server/**/*.js",
      "public/js/**/*.js"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "image-size": "^2.0.2",
    "keytar": "^7.9.0",
    "leaflet": "^1.9.4",
    "leaflet-environmental-layers": "^2.4.9",
    "leaflet.markercluster": "^1.5.3",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.1",
    "page-flip": "^2.0.7",
    "react-pageflip": "^2.0.3",
    "selenium-webdriver": "^4.38.0",
    "sharp": "^0.34.5",
    "validator": "^13.15.20",
    "winston": "^3.18.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^6.10.1",
    "archiver": "^7.0.1",
    "docdash": "^2.0.2",
    "eslint": "^9.39.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.5"
  }
}
